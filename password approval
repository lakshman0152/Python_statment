def user_password(password):                 #defining user_password function
    if len(password)<8:                      #if length of pasword is less than 8 characters, it prints "weak password
        return("weak password")
    elif len(password)>15:                   #else if ,length of password is greater than 15 characters, then it prints 
        return ("too big paasword")
    else:
    return("strong password")                # if password length is in between it prints "strong password"

password = input("enter ur password : ")     #by using "input" builtin function , user can enter is choice of password 
print(user_password(password))               #fuction call
